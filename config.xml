<?xml version="1.0" encoding="UTF-8"?>
<widget xmlns:tizen="http://tizen.org/ns/widgets" xmlns="http://www.w3.org/ns/widgets" id="http://tizen.oqee.tv/%PKG_NAME%" version="%PKG_VERSION%" viewmodes="maximized">
    <tizen:application id="%PKG_ID%.%PKG_NAME%" package="%PKG_ID%" required_version="2.3" />
    <tizen:profile name="tv-samsung" />
    <name>%PKG_NAME%</name>

    <icon src="icon.png" />
    <content src="index.html" />

    <feature name="http://tizen.org/feature/screen.size.normal.1080.1920" />
    <feature name="http://tizen.org/feature/network.ethernet" />

    <tizen:setting
            screen-orientation="landscape"
            context-menu="enable"
            background-support="disable"
            encryption="disable"
            install-location="auto"
            hwkey-event="enable" />

    <tizen:privilege name="http://tizen.org/privilege/application.info" />
    <tizen:privilege name="http://tizen.org/privilege/internet" />
    <tizen:privilege name="http://tizen.org/privilege/tv.inputdevice" />

    <tizen:privilege name="http://developer.samsung.com/privilege/productinfo" />

    <access origin="*" subdomains="true"></access>
</widget>
